{
	"name": "@react-loadable/revised",
	"version": "0.0.20",
	"description": "A higher order component for loading components with promises",
	"main": "lib/index.js",
	"author": "Tran Sang <tr@nsang.me>",
	"license": "MIT",
	"repository": "react-loadable/revised",
	"files": [
		"babel.js",
		"webpack.js",
		"babel.d.ts",
		"webpack.d.ts",
		"lib/**"
	],
	"scripts": {
		"test": "jest --coverage",
		"build": "tsc --downlevelIteration --jsx react --declaration --esModuleInterop --outDir ./lib/ src/* && tsc --declaration webpack.ts && tsc --declaration babel.ts",
		"clean": "rm -rf coverage lib example/dist node_modules/.cache",
		"start": "yarn build && webpack --mode=development && node example/dist/server/main.js",
		"prepublishOnly": "yarn test && yarn clean && yarn build"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/node": "^7.12.1",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-transform-async-to-generator": "^7.12.1",
		"@babel/plugin-transform-object-assign": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@babel/preset-typescript": "^7.12.1",
		"@types/node": "^14.11.10",
		"@types/react": "^16.9.53",
		"@types/webpack": "^4.41.22",
		"babel-jest": "^26.6.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"express": "^4.17.1",
		"jest": "^26.6.0",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-test-renderer": "^16.14.0",
		"regenerator-runtime": "^0.13.7",
		"typescript": "^4.0.3",
		"webpack": "^5.1.3",
		"webpack-cli": "^4.1.0",
		"webpack-node-externals": "^2.5.2"
	},
	"peerDependencies": {
		"react": "*"
	}
}
