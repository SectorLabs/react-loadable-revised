{
	"name": "@react-loadable/revised",
	"version": "0.0.5",
	"description": "A higher order component for loading components with promises",
	"main": "lib/index.js",
	"author": "Tran Sang <tr@nsang.me>",
	"license": "MIT",
	"repository": "react-loadable/revised",
	"files": [
		"babel.js",
		"webpack.js",
		"babel.d.ts",
		"webpack.d.ts",
		"lib/**"
	],
	"scripts": {
		"test": "jest --coverage",
		"build": "tsc --downlevelIteration --jsx react --declaration --esModuleInterop --outDir ./lib/ src/* && tsc --declaration webpack.ts && tsc --declaration babel.ts",
		"clean": "rm -rf coverage lib example/dist node_modules/.cache",
		"start": "yarn build && webpack --mode=development && node example/dist/server/main.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/cli": "^7.10.4",
		"@babel/core": "^7.10.4",
		"@babel/node": "^7.10.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-transform-async-to-generator": "^7.10.4",
		"@babel/plugin-transform-object-assign": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/node": "^14.0.23",
		"@types/react": "^16.9.43",
		"@types/webpack": "^4.41.21",
		"babel-jest": "^26.1.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"express": "^4.17.1",
		"jest": "^26.1.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-test-renderer": "^16.13.1",
		"regenerator-runtime": "^0.13.5",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-node-externals": "^2.5.0"
	},
	"peerDependencies": {
		"react": "*"
	}
}
